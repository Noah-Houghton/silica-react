{
  "name": "silica-react",
  "version": "0.0.1-alpha.1",
  "description": "Renderer Set and helper components for Silica-Django",
  "main": "lib/jsonforms-react-vanilla.cjs.js",
  "module": "lib/jsonforms-react-vanilla.esm.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "dev": "webpack --config ./webpack/webpack.dev.js && webpack-dev-server --config webpack/webpack.dev.js --env=dev --inline",
    "clean": "rimraf lib coverage dist .nyc_output 2> /dev/null",
    "report": "nyc report --reporter=html",
    "test": "jest --no-cache",
    "test-cov": "jest --no-cache --coverage",
    "doc": "typedoc --name 'JSON Forms Vanilla Renderers' --mode file --excludeExternals --theme ../../typedoc-jsonforms --out docs src"
  },
  "peerDependencies": {
    "@jsonforms/core": "3.0.0-beta.2",
    "@jsonforms/react": "3.0.0-beta.2",
    "react": "^16.12.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsonforms/core": "^3.0.0-beta.2",
    "@jsonforms/examples": "^2.5.2",
    "@jsonforms/react": "^3.0.0-beta.2",
    "@types/enzyme": "^3.10.3",
    "@types/react-highlight": "^0.12.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "copy-webpack-plugin": "^5.0.5",
    "enzyme": "^3.10.0",
    "jest": "^26.6.3",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "json-refs": "^3.0.15",
    "nyc": "^15.1.0",
    "react-dom": "^17.0.2",
    "react-highlight": "^0.14.0",
    "react-tabs": "^5.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-visualizer": "^5.4.1",
    "source-map-loader": "^0.2.4",
    "source-map-support": "0.5.16",
    "ts-loader": "^6.2.1",
    "ts-node": "^10.4.0",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.19.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "webpack-merge": "^5.8.0"
  }
}
